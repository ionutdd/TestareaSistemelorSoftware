@model FMInatorul.Models.Student

@{
    ViewData["Title"] = "Edit Year";
}

<h2>Edit Year</h2>

@using (Html.BeginForm(controllerName: "Students", actionName: "EditYear", method: FormMethod.Post,
     routeValues: new { id = Model.Id }))
{
    @Html.LabelFor(m => m.Year, "Anul de studiu")
    <br />
    @Html.EditorFor(m => m.Year, new { htmlAttributes = new { @class = "form-control" } })
    @Html.ValidationMessageFor(m => m.Year, null, new { @class = "text-danger" })
    <br />
    <br />
    <button type="submit" class="btn btn-primary">Save</button>
}

